            )





        };
    </script>
}






<h2>AdminDashBoard</h2>

<head>
    <link href="@Url.Content("~/Content/Background.css")" rel="stylesheet" type="text/css" />

</head>
<style>

    .square {
        float: left;
        position: relative;
        width: 20%;
        padding-bottom: 20%; /* = width for a 1:1 aspect ratio */
        margin: 1.66%;
        overflow: hidden;
    }

    .content {
        position: absolute;
        height: 80%; /* = 100% - 2*10% padding */
        width: 90%; /* = 100% - 2*5% padding */
        padding: 10% 5%;
    }
</style>

