@model MVCProject.Models.TreatmentReport

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>value</title>

    <style>
        body {
            background-image: url('~/Content/Images/main.jpg');
            background-repeat: no-repeat;
            background-attachment: fixed;
            background-size: 100% 100%;
        }
    </style>

    <script src="~/Scripts/jquery-3.4.1.min.js"></script>


</head>


<body style="background-color:cadetblue">
    <div>
        <h4>TreatmentReport</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayName("Doctor Name")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Doctor.FirstName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.HospitalDepartment.Department_Name)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.HospitalDepartment.Department_Name)
            </dd>

            <dt>
                @Html.DisplayName("Patient Name")
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Patient.FirstName)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.PatientMedicalReport.Diagnosis)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.PatientMedicalReport.Diagnosis)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Disease)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Disease)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Prescription)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Prescription)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.Date_Time)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Date_Time)
            </dd>

        </dl>
    </div>

</body>
</html>
